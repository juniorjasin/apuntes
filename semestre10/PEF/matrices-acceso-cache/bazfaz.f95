PROGRAM MAIN

IMPLICIT NONE

INTEGER :: M,N,J,I,NTIMES
PARAMETER (N=512, M=640, NTIMES=500)
DOUBLE PRECISION :: Q(N,M), R(M,N)

DO I=1,M
  DO J=1,N
    Q(J,I) = 1.0D0
    R(I,J) = 1.0D0
  ENDDO
ENDDO

DO I=1,NTIMES
  CALL BAZFAZ(Q,R,N,M)
ENDDO

END PROGRAM



SUBROUTINE BAZFAZ (Q,R,N,M)
IMPLICIT NONE
INTEGER :: M,N,J,I
DOUBLE PRECISION :: Q(N,M), R(M,N)

DO I=1,N
  DO J=1,M
    R(J,I) = Q(I,J) * R(J,I)
  ENDDO
ENDDO

END SUBROUTINE
